{
   "singleQuote": true,
   "tabWidth": 3,
   "bracketSpacing": true,
   "trailingComma": "all",
   "arrowParens": "always",
   "useTabs": false,
   "printWidth": 80,
   "semi": false,
   "endOfLine": "auto",
   "importOrder": ["^@/(.*)$", "^[./]"],
   "importOrderSeparation": true,
   "importOrderSortSpecifiers": true,
   "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
